<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
    default-autowire="byName" default-lazy-init="false">

    <bean id="jdbcLoginModule" class="au.gov.qld.fire.security.auth.JdbcLoginModule" />

    <bean id="permissionService" class="au.gov.qld.fire.service.impl.PermissionServiceImpl" />

    <bean id="securityPolicy" class="au.gov.qld.fire.security.SecurityPolicy" />

</beans>
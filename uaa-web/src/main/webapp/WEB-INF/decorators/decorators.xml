<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorators">
    <excludes>
        <pattern>/ckeditor*</pattern>
        <pattern>*/edit*</pattern>
        <pattern>/error.do</pattern>
        <pattern>*/error*</pattern>
        <pattern>*/export*</pattern>
        <pattern>*/find*</pattern>
        <pattern>*/get*</pattern>
        <pattern>*/print*</pattern>
        <pattern>*/blank.html</pattern>
    </excludes>
    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
    <decorator name="default-nonav" page="default-nonav.jsp">
        <pattern>/login.*</pattern>
        <pattern>/j_security_check.*</pattern>
        <pattern>/user/changePassword.*</pattern>
        <pattern>/user/changeContact.*</pattern>
    </decorator>
</decorators>